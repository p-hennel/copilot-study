    "@logtape/logtape": "^0.9.0",
    "@logtape/otel": "^0.3.0",
    "@melt-ui/svelte": "^0.86.6",
    "@rollup/plugin-json": "^6.1.0",
    "@socket.io/cluster-adapter": "^0.2.2",
    "@socket.io/pm2": "^5.3.0",
