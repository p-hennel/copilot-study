{
  "name": "copilot-survey",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build:server": "bun run build:web",
    "build:web": "bunx --bun vite build",
    "build:all": "bun run build:web && bun run build:crawler",
    "build:crawler": "bun build ./src/lib/crawler/runner/index.ts --outdir ./build/crawler --target bun",
    "build:hasher": "bun run build:hasher:unix64 && bun run build:hasher:win64 && bun run build:hasher:darwinx64 && bun run build:hasher:darwinarm",
    "build:hasher:unix64": "bun build --compile --target=bun-linux-x64-modern ./src/lib/hasher.ts --outfile ./build/hasher-unix-x64",
    "build:hasher:win64": "bun build --compile --target=bun-windows-x64-modern ./src/lib/hasher.ts --outfile ./build/hasher-win-x64",
    "build:hasher:darwinx64": "bun build --compile --target=bun-darwin-x64 ./src/lib/hasher.ts --outfile ./build/hasher-darwin-x64",
    "build:hasher:darwinarm": "bun build --compile --target=bun-darwin-arm64 ./src/lib/hasher.ts --outfile ./build/hasher-darwin-arm64",
    "preview": "bunx --bun vite preview",
    "serve": "bun --bun ./build/index.js",
    "prepare": "bunx --bun svelte-kit sync || echo ''",
    "check": "bunx --bun svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx --bun svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "bunx --bun prettier --write .",
    "lint": "bunx --bun prettier --check . && eslint .",
    "gql:codegen": "bunx --bun --env-file=.env graphql-codegen --config ./codegen.ts --overwrite",
    "auth:generate": "bunx @better-auth/cli generate --config src/lib/auth.ts --output src/lib/server/db/auth-schema.ts -y",
    "auth:update": "bun run auth:generate && bun run db:update",
    "db:update": "bun drizzle-kit generate && bun run db:migrate && bun run db:push",
    "db:push": "bunx --bun drizzle-kit push",
    "db:migrate": "bunx --bun drizzle-kit migrate",
    "db:studio": "bunx --bun drizzle-kit studio",
    "crawl": "bun --bun ./src/lib/crawler/runner/main.ts"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.0",
    "@inlang/paraglide-js": "^2.0.5",
    "@internationalized/date": "^3.7.0",
    "@lucide/svelte": "^0.484.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.17",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.17",
    "@testing-library/svelte": "^5.2.7",
    "@types/bun": "^1.2.8",
    "bits-ui": "^1.3.14",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.30.6",
    "embla-carousel-svelte": "^8.5.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-svelte": "^3.3.3",
    "formsnap": "^2.0.0",
    "globals": "^16.0.0",
    "lucide-svelte": "^0.479.0",
    "mode-watcher": "^0.5.1",
    "paneforge": "^1.0.0-next.4",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.25.3",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.1.5",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.24.0",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.0.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "^6.2.3",
    "zod": "^3.24.2"
  },
  "dependencies": {
    "@gitbeaker/rest": "^42.2.0",
    "@libsql/client": "^0.14.0",
    "@logtape/file": "^0.9.0",
    "@logtape/logtape": "^0.9.0",
    "@logtape/otel": "^0.3.0",
    "@rollup/plugin-json": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@urql/core": "^5.1.1",
    "@urql/exchange-retry": "^1.3.1",
    "better-auth": "^1.2.5",
    "build-url-ts": "^6.1.8",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "export-to-csv": "^1.4.0",
    "graphql": "^16.10.0",
    "js-yaml": "^4.1.0",
    "pm2": "^6.0.5",
    "rollup": "^4.37.0",
    "stream-transform": "^3.3.3",
    "svelte-exmarkdown": "^4.0.3",
    "svelte-time": "^2.0.1",
    "ts-node": "^10.9.2",
    "ulid": "^2.4.0"
  }
}
