{
  "name": "sst-copilot-study",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun --bun x vite dev",
    "build": "bun --bun x vite build",
    "preview": "bun --bun x vite preview",
    "serve": "bun --bun ./build/index.js",
    "prepare": "bun --bun x svelte-kit sync || echo ''",
    "check": "bun --bun x svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun --bun x svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "bun --bun x prettier --write .",
    "lint": "bun --bun x prettier --check . && eslint .",
    "gql:codegen": "bun --bun x --env-file=.env graphql-codegen --config ./codegen.ts --overwrite",
    "auth:generate": "bunx @better-auth/cli generate --config src/lib/auth.ts --output src/lib/server/db/auth-schema.ts -y",
    "auth:update": "bun run auth:generate && bun run db:update",
    "db:update": "bun drizzle-kit generate && bun run db:migrate && bun run db:push",
    "db:push": "bun --bun x drizzle-kit push",
    "db:migrate": "bun --bun x drizzle-kit migrate",
    "db:studio": "bun --bun x drizzle-kit studio"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@inlang/paraglide-js": "^2.0.8",
    "@internationalized/date": "^3.7.0",
    "@lucide/svelte": "^0.486.0",
    "@sveltejs/adapter-auto": "^5.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/enhanced-img": "^0.4.4",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.0",
    "@testing-library/svelte": "^5.2.7",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.2.8",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.17",
    "bits-ui": "^1.3.16",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.30.6",
    "embla-carousel-svelte": "^8.5.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^3.5.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "formsnap": "^2.0.0",
    "globals": "^16.0.0",
    "lucide-svelte": "^0.486.0",
    "mode-watcher": "^0.5.1",
    "paneforge": "^1.0.0-next.4",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sv": "^0.7.2",
    "svelte": "^5.25.6",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.1.5",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.24.0",
    "tailwind-merge": "^3.1.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.0",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
